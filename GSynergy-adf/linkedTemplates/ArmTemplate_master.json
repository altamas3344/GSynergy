{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GSynergy-adf"
		},
		"ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS'"
		},
		"SQL_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB'"
		},
		"ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gsynergy1adls.dfs.core.windows.net/"
		},
		"SQL_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "gyynergy.database.windows.net"
		},
		"SQL_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "GSynergy-DB"
		},
		"SQL_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "GSynergy-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_accountKey": {
						"value": "[parameters('ADLS_accountKey')]"
					},
					"SQL_DB_password": {
						"value": "[parameters('SQL_DB_password')]"
					},
					"ADLS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_properties_typeProperties_url')]"
					},
					"SQL_DB_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_properties_typeProperties_server')]"
					},
					"SQL_DB_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_properties_typeProperties_database')]"
					},
					"SQL_DB_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}